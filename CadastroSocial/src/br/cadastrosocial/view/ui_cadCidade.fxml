<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="281.0" prefWidth="459.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.cadastrosocial.controller.CadCidadeControler">
   <children>
            <Label layoutX="100.0" layoutY="14.0" style="-fx-font-size: 25;" text="Cadastro de Cidades" textAlignment="CENTER" />
      <JFXTextField fx:id="txtCidade" labelFloat="true" layoutX="14.0" layoutY="60.0" prefHeight="29.0" promptText="Cidade" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="60.0" />
      <JFXComboBox fx:id="cbEstados" labelFloat="true" layoutX="220.0" layoutY="60.0" onAction="#selecionaEstado" prefHeight="29.0" prefWidth="84.0" promptText="Estado" AnchorPane.topAnchor="60.0" />
      <Button fx:id="btnSaveCidade" layoutX="20.0" layoutY="115.0" mnemonicParsing="false" onAction="#saveCidade" text="Salvar" />
      <JFXCheckBox fx:id="ckCidade" checkedColor="#f78519" layoutX="217.0" layoutY="147.0" text="Fortaleza" unCheckedColor="#971818">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </JFXCheckBox>
      <JFXCheckBox fx:id="ckCidade2" layoutX="217.0" layoutY="199.0" text="Crato">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </JFXCheckBox>
   </children>
</AnchorPane>
